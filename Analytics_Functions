--********************* Deleting rows with Partition By *******************************

DELETE X from ( SELECT *, rn=Row_number() OVER (partition BY [Column1],[Column2 ORDER BY  [Column1],[Column2,[START DATE] DESC ) FROM [Table Name]) x x WHERE rn>1

--********************* Deleting rows with Partition By *******************************

delete x from (
SELECT *, rn=Row_number() OVER (partition BY [Name] ORDER BY [Emp Code] desc) 
       FROM   TABLE_NAME)x
	   		WHERE rn>1
